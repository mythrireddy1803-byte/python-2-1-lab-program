n1, n2 = 0, 1
count = 0
number = int(input('Enter No.of fibonacci numbers to be display:'))
if number < 0:
    print('(please enter positive numbers only ...... )')
elif number == 1:
    print('The fibonacci numbers upto', number)
    print(n1)
else:
    print('The first', number, 'Fibonacci numbers')
    while count < number:
        print(n1)
        n3 = n1 + n2
        n1 = n2
        n2 = n3
        count += 1
