def average_of_three(a, b, c):
    return (a + b + c) / 3.0
print(average_of_three(10, 20, 30))
print(average_of_three(10, 20, 30) + 10)
if average_of_three(10, 23, -40) < 0:
    print('invalid')
print(average_of_three(average_of_three(1, 2, 3), 5, 7))
print(average_of_three(1, 2, 3) * average_of_three(1, 2, 3))
